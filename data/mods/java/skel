public class laby_name_Ant extends Robot {

// Double forward slash is a single line comment

/* Slash star is the start of a comment that may be more than one line.
   Star slash ends the comment */
   
   
  public void laby_name_ant()
  {
    laby_name_say("Lets get out of here!");
    laby_name_right();
    laby_name_forward();
    laby_name_take();
    laby_name_left();
    laby_name_forward();
    laby_name_drop();
    laby_name_right();
    laby_name_forward();
    laby_name_left();
    laby_name_forward();
    laby_name_forward();
    laby_name_right();
    laby_name_escape();
  }

  void walkForwardFiveSteps(){
    laby_name_forward();
    laby_name_forward();
    laby_name_forward();
    laby_name_forward();
    laby_name_forward();
  }

  void sampleLookForThings(){

    if (look() == Web) {
     // ...
    }
    else if(look() == Void) {
    //  ...
    }
    else if(look() == Rock) {
    //  ...
    }
    else if(look() == Web) {
    //  ...
    }
    else if(look() == Wall) {
    //  ...
    }
    else if(look() == Exit) {
    //  ...
    }
    else {
    //  ...
    }



  }


